meta {
  name: Change password
  type: graphql
  seq: 4
}

post {
  url: {{host}}/graphql
  body: graphql
  auth: bearer
}

auth:bearer {
  token: {{token}}
}

body:graphql {
  mutation {
    changePassword(data: {
      newPassword: "Ismael12345"
      oldPassword: "Ismael123456"
    }) {
      id
    }
  }
}
