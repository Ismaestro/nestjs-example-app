meta {
  name: Catch Pokemon
  type: http
  seq: 7
}

post {
  url: {{host}}/{{version}}/user/pokemon/catch
  body: json
  auth: none
}

body:json {
  {
    "pokemonId": 1
  }
}

script:pre-request {
  const accessToken = bru.getEnvVar('accessToken');
  if (accessToken) {
    req.headers.cookie = `accessToken=${accessToken}`;
  }
}
