meta {
  name: Update user
  type: graphql
  seq: 2
}

post {
  url: {{host}}/graphql
  body: graphql
  auth: bearer
}

auth:bearer {
  token: {{token}}
}

body:graphql {
  mutation {
    updateUser(data: {
      firstname: "Ismael"
      language: "en"
    }) {
      id
      firstname
      language
    }
  }
}
